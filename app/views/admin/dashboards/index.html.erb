<div class="container">
  <div class="row">
    <div class="text-center" style='background-color: ##ABE1FA;'>
      <h1>全ユーザー一覧</h1>
        <ul>
          <% @users.each do |user| %>
            <li>
              <%= @user.name %> - <%= user.email %>
              <%= link_to '削除', admin_user_path(user), method: :delete, data: { confirm: '本当に削除しますか？'} 
            </li>
        </ul>
    </div>
  </div>
</div>